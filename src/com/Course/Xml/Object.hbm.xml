<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.Course.Pojo.Object" table="object" schema="qlhp">
        <id name="idOb">
            <column name="ID_Ob" sql-type="varchar(10)" length="10"/>
        </id>
        <property name="nameOb">
            <column name="name_Ob" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="credit">
            <column name="credit" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>